{
  "name": "@fullstack-boilerplate/web",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "next -p 3000",
    "build": "next build",
    "start": "next start",
    "serve": "next start -p $PORT",
    "typecheck": "tsc --noEmit",
    "generate": "DEBUG=true graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "lint:test": "eslint --cache --ext .ts,.tsx src",
    "prettify": "prettier --write src",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(js|jsx|json|ts|tsx|html)|!graphql.tsx": [
      "prettier --write"
    ],
    "*.(ts|tsx)|!graphql.tsx": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.2.7",
    "@chakra-ui/react": "^1.0.1",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@hookform/resolvers": "^1.0.0",
    "@sentry/react": "^5.27.4",
    "@sentry/tracing": "^5.27.4",
    "components": "link:./src/components",
    "cookie": "^0.4.0",
    "dayjs": "^1.8.23",
    "framer-motion": "^2.9.4",
    "graphql": "15.4.0",
    "lib": "link:./src/lib",
    "next": "^10.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.0.0",
    "react-hook-form": "^6.8.0",
    "react-icons": "^3.10.0",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.1",
    "@graphql-codegen/cli": "1.19.2",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.10",
    "@graphql-codegen/typescript-react-apollo": "2.1.1",
    "@types/cookie": "0.4.0",
    "@types/graphql": "14.2.3",
    "@types/jwt-decode": "2.2.1",
    "@types/node": "11.15.37",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/yup": "0.29.9",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0"
  }
}
