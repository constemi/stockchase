{
  "name": "@fullstack-boilerplate/web",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "BROWSER=none TSC_COMPILE_ON_ERROR=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "lint-staged": {
    "*.{json,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --ext .jsx,.js,.ts,.tsx src",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.0.0-james.33.0",
    "@apollo/react-hooks": "3.1.4",
    "@chakra-ui/core": "0.7.0",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@noquarter/hooks": "1.1.13",
    "@noquarter/utils": "1.0.8",
    "@reach/router": "1.3.3",
    "dayjs": "1.8.24",
    "emotion-theming": "10.0.27",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "4.10.2",
    "react-scripts": "3.4.1",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.2",
    "@graphql-codegen/typescript": "1.13.2",
    "@graphql-codegen/typescript-operations": "1.13.2",
    "@graphql-codegen/typescript-react-apollo": "1.13.2",
    "@types/graphql": "14.2.3",
    "@types/jest": "25.2.1",
    "@types/node": "12.12.35",
    "@types/reach__router": "1.3.4",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/yup": "0.26.36",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "graphql-tag.macro": "2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
