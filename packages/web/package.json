{
  "name": "@fullstack-boilerplate/web",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html --port 3000",
    "build": "rm -rf dist && NODE_ENV=production parcel build src/index.html",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "generate": "graphql-codegen",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix --cache --ext .ts,.tsx src",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@noquarter/hooks": "^1.1.11",
    "@noquarter/ui": "^3.0.0-alpha.1",
    "@noquarter/utils": "^1.0.6",
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.1.1",
    "react": "^16.9.0",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.9.0",
    "styled-media-query": "^2.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-operations": "1.7.0",
    "@graphql-codegen/typescript-react-apollo": "1.7.0",
    "@types/graphql": "14.2.3",
    "@types/node": "11.13.0",
    "@types/reach__router": "1.2.6",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/styled-components": "4.1.19",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "jest": "24.9.0",
    "jest-dom": "3.5.0",
    "parcel-bundler": "1.12.4",
    "react-testing-library": "7.0.1",
    "ts-jest": "24.1.0"
  }
}
