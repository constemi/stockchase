{
  "name": "@fullstack-boilerplate/web",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html --port 3000",
    "build": "rm -rf dist && NODE_ENV=production parcel build src/index.html",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "generate": "graphql-codegen",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix --cache --ext .ts,.tsx src",
      "git add"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.3.1",
    "graphql": "^14.1.1",
    "polished": "^3.0.2",
    "react": "^16.8.4",
    "react-apollo": "^2.5.5",
    "react-apollo-hooks": "^0.4.3",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3",
    "styled-media-query": "^2.1.1",
    "use-media": "^1.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.1.3",
    "@graphql-codegen/typescript": "1.1.3",
    "@graphql-codegen/typescript-operations": "1.1.3",
    "@graphql-codegen/typescript-react-apollo": "1.1.3",
    "@types/graphql": "14.2.0",
    "@types/node": "11.13.0",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.14",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "3.8.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "jest": "24.7.1",
    "jest-dom": "3.1.3",
    "parcel-bundler": "1.12.3",
    "react-testing-library": "6.0.4",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  }
}
