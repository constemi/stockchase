{
  "name": "@fullstack-boilerplate/web",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "BROWSER=none TSC_COMPILE_ON_ERROR=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "lint-staged": {
    "*.{json,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --ext .jsx,.js,.ts,.tsx src",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.0.2",
    "@apollo/react-hooks": "3.1.5",
    "@chakra-ui/core": "0.8.0",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@noquarter/hooks": "1.1.13",
    "@noquarter/utils": "1.0.8",
    "@reach/router": "1.3.4",
    "dayjs": "1.8.30",
    "emotion-theming": "10.0.27",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "5.7.2",
    "react-scripts": "3.4.1",
    "yup": "0.29.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.4",
    "@graphql-codegen/typescript": "1.17.4",
    "@graphql-codegen/typescript-operations": "1.17.4",
    "@graphql-codegen/typescript-react-apollo": "1.17.4",
    "@types/graphql": "14.2.3",
    "@types/jest": "26.0.5",
    "@types/node": "12.12.51",
    "@types/reach__router": "1.3.5",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/yup": "0.29.3",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "graphql-tag.macro": "2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
